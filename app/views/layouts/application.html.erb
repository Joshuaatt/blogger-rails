<!DOCTYPE html>
<html>
<head>
  <title>Blogger</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jquery', 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="row row-centered">
      <div class="col-md-8 col-centered">
        <h1>test</h1>
        <%= flash[:alert] %>
        <%= flash[:notice] %>
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
